URxvt.loginShell: true
URxvt.geometry: 80x50
URxvt.saveLines: 10000
URxvt.cutchars: "`\"'&()*,:;<=>?[\\]^{|}$"
URxvt.foreground: rgb:e9/e9/e9
URxvt.background: rgb:00/00/00
URxvt.color0: rgb:00/00/00
URxvt.color1: rgb:a8/00/00
URxvt.color2: rgb:00/a8/00
URxvt.color3: rgb:a8/54/00
URxvt.color4: rgb:00/00/a8
URxvt.color5: rgb:a8/00/a8
URxvt.color6: rgb:00/a8/a8
URxvt.color7: rgb:a8/a8/a8
URxvt.color8: rgb:54/54/54
URxvt.color9: rgb:fc/54/54
URxvt.color10: rgb:54/fc/54
URxvt.color11: rgb:fc/fc/54
URxvt.color12: rgb:54/54/fc
URxvt.color13: rgb:fc/54/fc
URxvt.color14: rgb:54/fc/fc
URxvt.color15: rgb:fc/fc/fc
URxvt.colorBD: rgb:fc/fc/fc
URxvt.font: xft:DejaVu Sans:size=17
URxvt.italicFont: xft:DejaVu Sans:size=17
URxvt.perl-ext-common: font-size,default,matcher
URxvt.keysym.C-Up:     perl:font-size:increase
URxvt.keysym.C-Down:   perl:font-size:decrease
URxvt.keysym.C-S-Up:   perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal
URxvt.scrollBar: false
URxvt.visualBell: false
URxvt.transparent: false
URxvt.fading: 15
URxvt.termName: rxvt-unicode-256color

URxvt.iso14755_52: False
URxvt.iso14755: False

! Map Contro-Shift-[hjkl] to escape sequences so they can be mapped in vim inside tmux
URxvt.keysym.Control-Shift-H: \033Ptmux;\033\033[72;5u\033\134
URxvt.keysym.Control-Shift-J: \033Ptmux;\033\033[74;5u\033\134
URxvt.keysym.Control-Shift-K: \033Ptmux;\033\033[75;5u\033\134
URxvt.keysym.Control-Shift-L: \033Ptmux;\033\033[76;5u\033\134

urxvt*url-launcher: firefox
urxvt.colorUL: #86a2be
urxvt*matcher.button: 1

XTerm*vt100.translations: #override \
  Ctrl ~Meta Shift <Key>h: string(0x1b) string("[72;5u") \n\
  Ctrl ~Meta Shift <Key>j: string(0x1b) string("[74;5u") \n\
  Ctrl ~Meta Shift <Key>k: string(0x1b) string("[75;5u") \n\
  Ctrl ~Meta Shift <Key>l: string(0x1b) string("[76;5u")

Xft.dpi: 192
